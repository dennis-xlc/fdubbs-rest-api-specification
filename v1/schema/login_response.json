{
    "type":"object",
    "$schema":"http://json-schema.org/draft-04/hyper-schema",
    "name":"login response",
    "description":"login response",
    "id":"login_response:v1",
    "properties":{
        "result_code":{
            "type":"string",
            "description":"result code",
            "required":true,
            "enum":[
                "SUCCESS",
                "USER_NOT_EXIST",
                "PASSWD_INCORRECT",
                "USER_ID_EMPTY",
                "PASSWD_EMPTY",
                "INTERNAL_ERROR"
            ]
        },
        "error_message":{
            "type":"string",
            "description":"error message"
        },
        "cookies":{
            "type":"array",
            "description":"cookies",
            "items":{
                "$ref":"cookie_key_value_pair.json"
            }
        }
    }
}